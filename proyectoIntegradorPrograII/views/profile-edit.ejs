<!DOCTYPE html>
<html lang="es">
	<%- include("./partials/head") %>
<body>
	<!-- Header -->
	<%- include("./partials/headerLogueado") %> 
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Editar mi perfil</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<h2 class="col-12"></h2>
			<div>
				<article>
					<form action="/index" method="get">
						<div>
						<input type="email" name="email" placeholder="Email">
						</div>
			
						<br>
			
						<div>
							<input type="text" name="usuario" placeholder="Usuario">
						</div>
			
						<br>
			
						<div>
							<input type="text" name="contrasena" placeholder="Contrasena">
						</div>
			
						<br>
			
						<div>
							<input type="date" name="fechaNacimiento" placeholder="Fecha de nacimiento">
						</div>
			
						<br>
			
						<div>
							<input type="number" name="documento" placeholder="Numero de documento">
						</div>
			
						<br>
			
						<div>
							<input type="file" name="fotoPerfil" placeholder="Foto de perfil">
						</div>
			
						<br>
			
						<div>
						<button type="submit">Enviar</button>
						</div>
					</form>
				</article>
			</div>


	<!-- footer -->
	<%- include('./partials/footer') %>
	<!-- /footer -->
</body>

</html>