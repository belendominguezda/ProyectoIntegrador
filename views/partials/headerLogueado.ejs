	<!-- Header LOGEADO-->
	<header class="main-header">
		<div class="container">
			<div class="row align-items-center">
				<div class="col-5 col-md-2">
					<a href="/" class="main-header_home-link">
						<img src="/images/LOGO.png">
					</a>
				</div>
	
				<div class="col-7 col-md-6">
					<form action="/" method="GET" class="search-form">
						<input type="text" name="search" placeholder="Buscar productos, artistas y más" class="search-form_input">
						<button type="submit" class="search-form_button"><i class="fas fa-search"></i></button>
					</form>
				</div>
	
				<div class="col-12 col-md-4">
					<a class="main-header_credit-link">
						Viajá a la época que desees <i class="fas fa-heart" style = "color: #c3124a;"></i>
					</a>
				</div>
			</div>
	
			<button class="btn-toggle-navbar">
				<i class="fas fa-bars"></i>
			</button>
	
			<nav class="main-navbar">
				<ul class="left-navbar">
					<li><a href="/" class="navegacion">Todos los productos</a></li>
					<li><a href="/products/product-add" class="navegacion">Cargar productos</a></li>
				</ul>
	
				<% for (let i=0; i<informacion.length; i++){ %>
				<ul class="right-navbar">
					<li><a href="/user/profile" class="navegacion">
							<%= informacion[i].usuario %> <i class="fas fa-user"></i>
						</a>
					</li>
				<% } %>
					<li>
						<form action="/" method="GET">
							<button class="logout navegacion" type="submit">
								Cerrar sesión <i class="fas fa-sign-in-alt"></i>
							</button>
						</form>
					</li>
					<!-- Solo deslogueado--> <li>
						<a href="/user/register" class="navegacion">Creá tu cuenta <i class="far fa-address-card"></i>
						</a>
					</li>
				</ul>
			</nav>
		</div>
	</header>
	<!-- /Header LOGEADO -->